          </view>
        </view> -->
      <!-- end 材料上传 -->
			<!-- end 材料上传 -->

				<!-- 提交 -->
			 <view class="btnSubmit">
        <button class="btn-compl" formType="submit" style="background-image: url('https://www.nbxwx.cn/SpringBootJT/show/images?fileName=btnbg.png');background-size:100% 100%; border:none; color:white;" plain="false">提交</button>
      </view>
			<!--end 提交 -->
		</form>
</view>


