page{
  width: 100%;
  height: 100%;
}
.box{
  width: 100%;
  height: 100%;
  word-break:break-all;
}
.boxItems{
  width: 90%;
  height: 90%;
  margin: 5%;
}

.item{
  width: 90%;
  height: 40px;
  line-height: 40px;
  text-align: left;
  font-size: 14px;
  font-weight: 500;
  color:#000;
  padding: 0px 5%;
  border-bottom: 1px solid #faf9fe;
}
.itemActive{
  background: linear-gradient(#f0f9ff, #a3daff);
}

.tj{
  width: 100%;
  height: 50px;
  /* background: yellow; */
}
.tjInputSty{
  width: 80%;
  height: 50px;
  line-height: 50px;
  position: relative;
  float: left;
  /* background: yellowgreen; */
}
.tjInputSty input{
  width: 94%;
  height: 35px;
  line-height: 35px;
  font-size: 14px;
  position: absolute;
  top:10%;
  padding-left:5%;
  border-radius: 5px;
  border:1px  solid #ededed;
}
.tjButtonSty{
  width: 18%;
  height: 50px;
  margin-left:2%;
  /* background: red; */
  float: right;
  position: relative;
}
.tjButtonSty button{
  width: 100%;
  height: 35px;
  line-height: 35px;
  font-size: 14px;
  position: absolute;
  top:10%;
  color: white;
  background: linear-gradient(to right,#326cfb, #00d0ff);
}

.nullSty{
  width: 100%;
  font-size:14px;
  text-align: center;
  margin-top: 30px;
  color: #000;
}